{
  "name": "koa-api-starter",
  "version": "2.3.0",
  "description": "Koa api",
  "private": false,
  "main": "src/app.js",
  "author": "Paralect",
  "license": "MIT",
  "keywords": [
    "koa",
    "rest api",
    "paralect"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paralect/koa-api-starter.git"
  },
  "scripts": {
    "build-assets": "mjml ./src/assets/emails/*.mjml -o ./src/assets/emails/dist/",
    "build-emails": "babel src/emails --out-dir src/emails/compiled",
    "dev": "nodemon --watch src src/app.js",
    "init-project": "./bin/init-project.sh",
    "start": "node src/app.js",
    "migrate": "node ./src/migrator.js",
    "schedule-dev": "nodemon --watch ./src ./src/scheduler ./src/scheduler.js",
    "schedule": "node ./src/scheduler.js",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@koa/cors": "3.1.0",
    "@koa/multer": "3.0.0",
    "@koa/router": "10.1.1",
    "@paralect/node-mongo": "2.1.1",
    "@react-email/components": "^0.0.20",
    "@react-email/render": "^0.0.16",
    "@sendgrid/mail": "7.6.1",
    "@socket.io/redis-adapter": "7.1.0",
    "@socket.io/redis-emitter": "4.1.1",
    "app-module-path": "2.2.0",
    "aws-sdk": "2.1080.0",
    "bcryptjs": "2.4.3",
    "dotenv": "16.0.0",
    "eslint-config-prettier": "8.5.0",
    "handlebars": "4.7.7",
    "joi": "^17.13.3",
    "koa": "2.13.4",
    "koa-bodyparser": "4.3.0",
    "koa-helmet": "6.1.0",
    "koa-logger": "3.2.1",
    "koa-mount": "4.0.0",
    "koa-qs": "3.0.0",
    "lodash": "4.17.21",
    "mjml": "4.12.0",
    "mkdirp": "1.0.4",
    "moment": "2.29.1",
    "moment-duration-format": "2.3.2",
    "multer": "1.4.4",
    "node-schedule": "2.1.0",
    "nodemailer": "^6.9.14",
    "nodemon": "2.0.15",
    "prettier": "2.6.2",
    "psl": "1.8.0",
    "redis": "3.1.2",
    "require-dir": "1.2.0",
    "slug": "^9.1.0",
    "socket.io": "4.4.1",
    "socket.io-emitter": "3.2.0",
    "tail": "2.2.4",
    "winston": "3.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "eslint": "8.9.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.25.4",
    "husky": "7.0.4",
    "lint-staged": "12.3.4"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    ".js": [
      "npm run lint:fix",
      "npm run lint"
    ]
  }
}
