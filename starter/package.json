{
  "name": "koa-api-starter",
  "version": "2.3.0",
  "description": "Koa api",
  "private": false,
  "main": "src/app.js",
  "author": "Paralect",
  "license": "MIT",
  "keywords": [
    "koa",
    "rest api",
    "paralect"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paralect/koa-api-starter.git"
  },
  "scripts": {
    "build-assets": "mjml ./src/assets/emails/*.mjml -o ./src/assets/emails/dist/",
    "build-emails": "babel src/emails --out-dir src/emails/compiled",
    "dev": "tsx --watch src/app.js",
    "init-project": "./bin/init-project.sh",
    "start": "node src/app.js",
    "migrate": "node ./src/migrator.js",
    "schedule-dev": "nodemon --watch ./src ./src/scheduler ./src/scheduler.js",
    "schedule": "node ./src/scheduler.js",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@koa/cors": "5.0.0",
    "@koa/multer": "3.0.2",
    "@koa/router": "12.0.1",
    "@paralect/node-mongo": "3.2.0",
    "@react-email/components": "^0.0.21",
    "@react-email/render": "^0.0.16",
    "@sendgrid/mail": "8.1.3",
    "@socket.io/redis-adapter": "8.3.0",
    "@socket.io/redis-emitter": "5.1.0",
    "app-module-path": "2.2.0",
    "aws-sdk": "2.1661.0",
    "bcryptjs": "2.4.3",
    "bullmq": "^5.10.3",
    "dotenv": "16.4.5",
    "eslint-config-prettier": "9.1.0",
    "handlebars": "4.7.8",
    "joi": "^17.13.3",
    "koa": "2.15.3",
    "koa-bodyparser": "4.4.1",
    "koa-helmet": "7.0.2",
    "koa-logger": "3.2.1",
    "koa-mount": "4.0.0",
    "koa-qs": "3.0.0",
    "lodash": "4.17.21",
    "mjml": "4.15.3",
    "mkdirp": "3.0.1",
    "moment": "2.30.1",
    "moment-duration-format": "2.3.2",
    "monk": "^7.3.4",
    "multer": "1.4.4",
    "node-schedule": "2.1.1",
    "nodemailer": "^6.9.14",
    "prettier": "3.3.3",
    "psl": "1.9.0",
    "redis": "^3.1.2",
    "require-dir": "1.2.0",
    "slug": "^9.1.0",
    "socket.io": "4.7.5",
    "socket.io-emitter": "3.2.0",
    "tail": "2.2.6",
    "winston": "3.13.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@types/node": "^20.14.11",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "9.1.1",
    "lint-staged": "15.2.7",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    ".js": [
      "npm run lint:fix",
      "npm run lint"
    ]
  }
}
